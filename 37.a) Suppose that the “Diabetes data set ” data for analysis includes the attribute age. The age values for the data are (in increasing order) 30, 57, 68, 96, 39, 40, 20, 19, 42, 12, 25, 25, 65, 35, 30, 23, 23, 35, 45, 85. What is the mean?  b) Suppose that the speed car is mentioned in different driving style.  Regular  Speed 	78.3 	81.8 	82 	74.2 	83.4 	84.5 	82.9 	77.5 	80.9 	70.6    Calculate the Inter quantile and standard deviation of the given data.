
ages <- c(30, 57, 68, 96, 39, 40, 20, 19, 42, 12, 25, 25, 65, 35, 30, 23, 23, 35, 45, 85)
mean_age <- mean(ages)
cat("Mean Age:", mean_age, "\n")


speeds <- c(78.3, 81.8, 82.0, 74.2, 83.4, 84.5, 82.9, 77.5, 80.9, 70.6)
iqr_speeds <- IQR(speeds)
sd_speeds <- sd(speeds)

cat("Interquartile Range (IQR) of Speeds:", iqr_speeds, "\n")
cat("Standard Deviation of Speeds:", sd_speeds, "\n")
