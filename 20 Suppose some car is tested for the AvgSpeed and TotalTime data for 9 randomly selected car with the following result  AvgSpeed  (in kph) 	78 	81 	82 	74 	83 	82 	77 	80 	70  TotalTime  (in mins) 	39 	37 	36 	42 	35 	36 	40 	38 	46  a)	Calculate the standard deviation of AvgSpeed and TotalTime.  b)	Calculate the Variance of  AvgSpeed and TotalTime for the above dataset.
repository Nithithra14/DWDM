
data <- data.frame(
  AvgSpeed = c(78, 81, 82, 74, 83, 82, 77, 80, 70),
  TotalTime = c(39, 37, 36, 42, 35, 36, 40, 38, 46)
)


std_dev_avg_speed <- sd(data$AvgSpeed)
std_dev_total_time <- sd(data$TotalTime)


cat("Standard Deviation of AvgSpeed:", std_dev_avg_speed, "\n")
cat("Standard Deviation of TotalTime:", std_dev_total_time, "\n")


var_avg_speed <- var(data$AvgSpeed)
var_total_time <- var(data$TotalTime)


cat("Variance of AvgSpeed:", var_avg_speed, "\n")
cat("Variance of TotalTime:", var_total_time, "\n")
